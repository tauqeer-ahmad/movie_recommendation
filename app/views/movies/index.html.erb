<h1>Movies</h1>
<ul class="thumbnails">
  <% @movies.each do |movie| %>
    <li>
      <div class="thumbnail">
        <h3><%= movie.display_title %></h3>
        <div class="row-fluid">
          <div class="span2">
            Year: <%= movie.display_year %>
          </div>
          <div class="span10">
            Genres: <%= movie.display_genres %>
          </div>
        </div>
      </div>
    </li>
  <% end %>
</ul>
